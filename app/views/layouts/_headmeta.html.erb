<link rel="shortcut icon" href="/favicon.ico">
<link rel="apple-touch-icon" href="/apple-touch-icon.png">

<script type="text/javascript" src="http://fast.fonts.com/jsapi/85d9a444-be94-4252-865b-af10fb926300.js"></script>

<%#= ******** CSS ********** %>
<%= stylesheet_link_tag 'verisk' %>

